{
  "name": "reine/yii2-widget-datatables",
  "version": "0.1.1",
  "description": "DataTables widget for Yii2 with Bootstrap 4",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "yii2",
    "extension",
    "jquery",
    "datatables",
    "table",
    "javascript",
    "library",
    "bootstrap4"
  ],
  "license": "MIT",
  "support": {
    "source": "https://github.com/reine/yii2-widget-datatables"
  },
  "authors": [
    {
      "name": "Federico NicolÃ¡s Motta",
      "email": "fedemotta@gmail.com"
    },
    {
      "name": "Stanislav",
      "email": "delfi89@gmail.com"
    },
    {
      "name": "Pasquale Pellicani",
      "email": "pellicanipasquale@gmail.com"
    },
    {
      "name": "Reina Sison",
      "email": "reawakened.phoenix@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/jquery": ">= 1.7.0",
    "bower-asset/datatables.net": "^1.10.20",
    "bower-asset/datatables.net-bs4": "^1.10.20",
    "bower-asset/datatables.net-select-bs4": "^1.3.1",
    "bower-asset/datatables.net-buttons-bs4": "^1.6.1",
    "bower-asset/jszip": "*",
    "bower-asset/pdfmake": "*"
  },
  "autoload": {
    "psr-4": {
      "reine\\datatables\\": ""
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}