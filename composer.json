{
  "name": "nightycrawl/yii2-widget-datatables-bs5",
  "version": "1.1",
  "description": "DataTables widget for Yii2 with Bootstrap 5",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "yii2",
    "extension",
    "jquery",
    "datatables",
    "table",
    "javascript",
    "library",
    "bootstrap5"
  ],
  "license": "MIT",
  "support": {
    "source": "https://github.com/nightycrawl/yii2-widget-datatables"
  },
  "authors": [
    {
      "name": "Federico NicolÃ¡s Motta",
      "email": "fedemotta@gmail.com"
    },
    {
      "name": "Stanislav",
      "email": "delfi89@gmail.com"
    },
    {
      "name": "Pasquale Pellicani",
      "email": "pellicanipasquale@gmail.com"
    },
    {
      "name": "Reina Sison",
      "email": "reawakened.phoenix@gmail.com"
    },
    {
      "name": "NightyCrawl",
      "email": "lads.lard.18@icloud.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "bower-asset/jquery": ">= 1.7.0",
    "bower-asset/datatables.net": "^2.3.0",
    "npm-asset/datatables.net-bs5": "^2.3.1",
    "npm-asset/datatables.net-select-bs5": "^3.0.0",
    "npm-asset/datatables.net-buttons-bs5": "^3.2.2",
    "bower-asset/jszip": "*",
    "bower-asset/pdfmake": "*"
  },
  "autoload": {
    "psr-4": {
      "nightycrawl\\datatables\\": ""
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
